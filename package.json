{
  "name": "simple-react-express-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "cd client && npm run dev",
    "dev:server": "node server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "setup": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}